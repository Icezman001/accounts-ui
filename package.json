{
  "name": "accounts-ui",
  "version": "1.0.0",
  "description": "Zesty.io account management application",
  "repository": {
    "git": "https://github.com/zesty-io/accounts-ui"
  },
  "main": "webpack.config.js",
  "scripts": {
    "precommit": "pretty-quick --staged",
    "serve": "./node_modules/.bin/serve ./build --port 6006 --single",
    "prestart": "npm install",
    "start": "npm run build-dev && node bin/start",
    "build-dev": "export NODE_ENV=\"development\" && node bin/build",
    "build-stage": "export NODE_ENV=\"stage\" && node bin/build",
    "build-prod": "export NODE_ENV=\"production\" && node bin/build",
    "test:dev": "cypress open",
    "test": "cypress run --browser chrome"
  },
  "author": "Zesty.io<developers@zesty.io>",
  "contributors": [
    "Grant Glidewell<grant.glidewell@zesty.io>",
    "Stuart Runyan<stuart@zesty.io>"
  ],
  "license": "All Rights Reserved",
  "dependencies": {
    "classnames": "2.2.6",
    "core-js": "2.5.7",
    "js-cookie": "2.2.0",
    "qs": "6.5.1",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-redux": "5.0.7",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "css-loader": "1.0.1",
    "cypress": "3.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "husky": "1.3.1",
    "less": "3.9.0",
    "less-loader": "4.1.0",
    "prettier": "1.17.0",
    "pretty-quick": "1.10.0",
    "redux-logger": "3.0.6",
    "serve": "11.0.0",
    "webpack": "3.8.1"
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "singleQuote": true,
    "tabWidth": 2,
    "semi": false
  },
  "false": {}
}
